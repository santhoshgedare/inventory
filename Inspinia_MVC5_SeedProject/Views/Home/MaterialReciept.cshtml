<div class="modal fade in" id="myModal6" tabindex="-1" role="dialog" aria-hidden="true" style="display: none;">
    <div class="modal-dialog modal-sm">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal"><span aria-hidden="true">×</span><span class="sr-only">Close</span></button>
                <h4 class="modal-title">Accept Quantity</h4>
            </div>
            <div class="modal-body">
               <label class="control-label">Enter item Quantity you wish to accept:</label>
                <input type="number" class="form-control" />
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-primary btn-sm" data-dissmiss="modal">Accept</button>
            </div>
        </div>
    </div>
