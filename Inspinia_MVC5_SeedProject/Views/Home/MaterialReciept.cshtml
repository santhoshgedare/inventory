
@{
    ViewBag.Title = "MaterialReciept";
}


<!-- Section Name1 -->
<section class="row wrapper border-bottom white-bg page-heading">

    <div class="col-lg-12 col-md-12 col-sm-2 col-xs-12">
        <h3 class="text-uppercase">
           Material Receipt
        </h3>
    </div>
</section>
<!-- Section Name2 -->
<section class="wrapper wrapper-content animated fadeInRight">
    <div class="row">
        <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
            <div class="ibox float-e-margins">
               
                <div class="ibox-content">
                    <div class="row">
                        <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                            <div class="row">
                                <div class="form-group col-lg-4">
                                    <label class="control-label">INVOICE NO:</label>
                                    <input type="text" class="form-control" id="txtInvoiceNo" placeholder="Invoice Number"/>
                                </div>
                                <div class="form-group col-lg-4">
                                    <label class="control-label">PO NO:</label>
                                    <input type="text" class="form-control" id="txtPONo" placeholder="Purchase Order Number"/>
                                </div>
                                <div class="form-group col-lg-4">
                                    <label class="control-label">CHALLAN NO:</label>
                                    <input type="text" class="form-control" id="txtChallaneNo" placeholder="Challan Number"/>
                                </div>
                                <div class="form-group col-lg-4">
                                    <label class="control-label">INVOICE DATE:</label>
                                    <input type="text" class="form-control" id="txtInvoiceNo" placeholder="Invoice Date"/>
                                </div>
                                <div class="form-group col-lg-4">
                                    <label class="control-label">PO DATE:</label>
                                    <input type="text" class="form-control" id="txtInvoiceNo" placeholder="Purchase Order Date"/>
                                </div>
                                <div class="form-group col-lg-4">
                                    <label class="control-label">CHALLAN DATE:</label>
                                    <input type="text" class="form-control" id="txtInvoiceNo" />
                                </div>
                                <div class="form-group col-lg-4">
                                    <label class="control-label">SUPPLILER NAME:</label>
                                    <input type="text" class="form-control" id="txtInvoiceNo" placeholder="Supplier Name"/>
                                </div>
                                <div class="form-group col-lg-4">
                                    <label class="control-label">SUPPLILER ID:</label>
                                    <input type="text" class="form-control" id="txtInvoiceNo" placeholder="Suppler ID"/>
                                </div>
                                <div class="form-group col-lg-4">
                                    <label class="control-label">GST NO:</label>
                                    <input type="text" class="form-control" id="txtInvoiceNo" placeholder="GST Number"/>
                                </div>
                                <div class="col-lg-12 text-center">
                                    <button class="btn btn-primary" id="btnSubmit">Submit</button>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div class="row">
        <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
            <div class="ibox float-e-margins">
                <div class="ibox-title">
