
@{
    ViewBag.Title = "StockTransfer";
}


<!-- Section Name1 -->
<section class="row wrapper border-bottom white-bg page-heading">

    <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
        <h3 class="text-uppercase">
           stock trasnsfer
        </h3>
    </div>
</section>
<!-- Section Name2 -->
<section class="wrapper wrapper-content animated fadeInRight">
    <div class="row">
        <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
            <div class="ibox float-e-margins">
                <div class="ibox-title">
                    <div class="row">
                        <div class="col-lg-3 col-md-4 col-sm-4 col-xs-4">
                            <select class="form-control input-sm" id="selItem">
                                <option>Item</option>
                                <option>Quantity From</option>
                                <option>Quantity To</option>
                                <option>Location</option>
                                <option>Category</option>
                                <option>Last Vendor</option>
                                <option>Description</option>
                                <option>Add</option>
                                <option>Remove</option>
                            </select>
                            </div>
                        <div class="col-lg-4 col-md-4 col-sm-4 col-xs-4">
                            <select class="form-control input-sm" id="selLocFrom">
                                <option>Delhi</option>
                                <option>Kolkatta</option>
                                <option>Mumbai</option>
                                <option>Bangalore</option>
                                <option>Kochin</option>
                                <option>Chennai</option>
                            </select>
                            </div>
                        <div class="col-lg-4 col-md-4 col-sm-4 col-xs-4">
                            <select class="form-control input-sm" id="selLocTo">
                                <option>Delhi</option>
                                <option>Kolkatta</option>
                                <option>Mumbai</option>
                                <option>Bangalore</option>
                                <option>Kochin</option>
                                <option>Chennai</option>
                            </select>
                        </div>
                        <div class="col-lg-1 text-left">
                            <button class="btn btn-primary btn-sm">Search </button>
                        </div>
                    </div>
                </div>
                <div class="ibox-content">
                    <div class="">
                        <table class="table table-hover table-bordered">
                            <thead>
                                <tr>
                                    <th>Item</th>
                                    <th>Name</th>
                                    <th>Description</th>
                                    <th>Category</th>
                                    <th>Quantity</th>
                                    <th>UOM</th>
                                    <th>From</th>
                                    <th>To</th>

                                </tr>
                            </thead>
                            <tbody>
                                <tr>
                                    <td>TV</td>
                                    <td>Sony</td>
                                    <td>LED FLat</td>
                                    <td>Electronics</td>
                                    <td>6</td>
                                    <td>Number</td>
                                    <td>Bangalore</td>
                                    <td>Delhi</td>
                                </tr>
                                <tr>
                                    <td>Sport Shoes</td>
                                    <td>Adidas</td>
                                    <td>Soccer Boot</td>
                                    <td>Sports</td>
                                    <td>20</td>
                                    <td>Pairs</td>
                                    <td>Chennai</td>
                                    <td>Mumbai</td>
                                </tr>
                            </tbody>
                        </table>
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>