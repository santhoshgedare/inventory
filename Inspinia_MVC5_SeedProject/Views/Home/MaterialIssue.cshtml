

@{
    ViewBag.Title = "Material Issue";
}

<!-- Section Name1 -->
<section class="row wrapper border-bottom white-bg page-heading">
    <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12 m-t-md">
        <h3 class="text-uppercase">
            Material issue
        </h3>
    </div>
</section>
<!-- Section Name2 -->
<section class="wrapper wrapper-content animated fadeInRight">
    <div class="row">
        <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
            <div class="ibox float-e-margins">
                <div class="ibox-title">
                    <div class="row">
                        <div class="col-lg-8 col-md-8 col-sm-8 col-xs-12">
                            <h5>
                                Section Heading will come here
                            </h5>
                        </div>
                        <div class="col-lg-4 col-md-4 col-sm-4 col-xs-12 pull-right">
                            <div class="ibox-tools">
                                Search Textbox or any Buttons will come here
                            </div>
                        </div>
                    </div>
                </div>
                <div class="ibox-content">
                    <div class="row">
                        <form>
                            <div class="form-group col-lg-2">
                                <label class="control-label">Issue To:</label>
                            </div>
                            <div class="form-group col-lg-3 col-md-5 col-sm-6 col-xs-12">
                                <input type="text" class="form-control" id="txtIssueTo" placeholder="Enter Employee No." />
                            </div>
                            <div class="form-group col-lg-3 col-md-5 col-sm-6 col-xs-12">
                                <input type="text" class="form-control" id="txtIssueTo" placeholder="Enter Employee Name" />
                            </div>
                            <div class="form-group col-lg-3 col-md-5 col-sm-6 col-xs-12">
                                <input type="text" class="form-control" id="txtIssueTo" placeholder="Enter Project Name" />
                            </div>
                        </form>
                    </div>
                </div>

            </div>
        </div>
    </div>
    <div class="row">
        <div class="col-lg-4 col-md-4 col-sm-4 col-xs-12">
            <div class="ibox float-e-margins">
                <div class="ibox-content">

                </div>
            </div>
        </div>
        <div class="col-lg-8 col-md-8 col-sm-8 col-xs-12">
            <div class="ibox float-e-margins">
                <div class="ibox-content">

                </div>
            </div>
        </div>
    </div>
</section>

